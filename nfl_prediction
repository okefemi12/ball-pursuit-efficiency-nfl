e).","metadata":{}},{"cell_type":"code","source":"# Distance closed to ball landing spot (True closing distance)\nval_train_rows['D_closed_to_ball_true'] = val_train_rows['dist_ball_last'] - calculate_distance(\n    val_train_rows['x_true'], \n    val_train_rows['y_true'], \n    val_train_rows['ball_land_x'], \n    val_train_rows['ball_land_y']\n)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-17T21:37:53.871655Z","iopub.execute_input":"2025-12-17T21:37:53.872011Z","iopub.status.idle":"2025-12-17T21:37:53.879699Z","shell.execute_reply.started":"2025-12-17T21:37:53.871987Z","shell.execute_reply":"2025-12-17T21:37:53.878411Z"}},"outputs":[],"execution_count":120},{"cell_type":"code","source":"# The BPE Metric: How much distance did they *actually* move towards the ball, compared to the total distance they moved.\n# This penalizes wasted lateral movement.\n# We also include a factor for how close the actual position was to the predicted/ideal position.\nval_train_rows['Efficiency_Ratio'] = val_train_rows['D_closed_to_ball_true'] / (val_train_rows['D_actual'] + 1e-6) # prevent divide by zero\n\n# Calculate penalty for prediction error (model uncertainty)\n# L2 error between true position and predicted position (higher error = lower BPE)\nval_train_rows['Prediction_Error'] = calculate_distance(\n    val_train_rows['x_true'], \n    val_train_rows['y_true'], \n    val_train_rows['x_pred'], \n    val_train_rows['y_pred']\n)\n\n# Final BPE Score (Higher is better)\n# Range approx 0 to 1, where 1 is perfect closing efficiency with zero prediction error.\nval_train_rows['BPE'] = val_train_rows['Efficiency_Ratio'] * np.exp(-val_train_rows['Prediction_Error'] / ensemble_rmse)\nval_train_rows['BPE'] = np.clip(val_train_rows['BPE'], 0, 1.5) # Clip to reasonable range for stability\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-17T21:38:01.375217Z","iopub.execute_input":"2025-12-17T21:38:01.375573Z","iopub.status.idle":"2025-12-17T21: